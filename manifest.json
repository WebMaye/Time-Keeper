{
    "manifest_version": 3,
    "name": "Time Keeper",
    "version": "1.0",
    "description": "Never miss a meeting across time zones.",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "geolocation",
      "storage"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}